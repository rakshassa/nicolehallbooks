<div class="col-md-2"  style="margin: 5px; background-color:#66ccff; border-style: solid; border-width: 1px; border-color: black; border-radius: 5px;">
  <div class="panel-heading">
    <h4 class="panel-title">
      <a data-toggle="collapse" data-parent="#accordion" href="#collapse_<%= name %>">
        <span class="fa fa-plus-square pull-left"></span>
        <%= name %>
      </a>
    </h4>
  </div>
  <hr class="thin_hr" />
  <div id="collapse_<%= name %>" class="panel-collapse collapse in">
    <div class="panel-body">
      <%= render 'stat_mods_table', mods: data %>
    </div>
  </div>
</div>
